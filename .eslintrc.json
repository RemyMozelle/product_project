{
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "prettier",
        "jest"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-plusplus": "off",
        "import/prefer-default-export": "off"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    }
  }
  